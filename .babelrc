{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
      "production": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel"]
      },
      "development": {
          "plugins": ["@babel/plugin-proposal-object-rest-spread", "styled-jsx/babel"]
      },
      "test": {
          "plugins": ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css"}, "antd"]
      }
  }
}
